pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello'
                sleep 4
            }
        }
         stage('build') {
           steps {
                echo 'Hello build'
                sleep 4
            }
        }  
         stage('test') {
           steps {
                echo 'test'
                sleep 4
            }
        }
         stage('deploy') {
           steps {
                echo 'deploy'
                sleep 4
            }
        }
    }
}
